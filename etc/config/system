config system
        option ttylogin '0'
        option log_size '64'
        option urandom_seed '0'
        option timezone 'CET-1CEST,M3.5.0,M10.5.0/3'
        option pretty_hostname 'ff_ungerwechs'
        option hostname 'ffungerwechs'

config timeserver 'ntp'
        option enabled '1'
        option enable_server '0'
        list server 'ntp1.infra.fftdf'
        list server 'ntp2.infra.fftdf'
        list server '0.de.pool.ntp.org'
        list server '1.de.pool.ntp.org'

config led 'led_wlan'
        option name 'WLAN'
        option sysfs 'gl-mifi:green:wlan'
        option trigger 'phy0tpt'

config led 'led_wan'
        option name 'WAN'
        option sysfs 'gl-mifi:green:wan'
        option trigger 'netdev'
        option mode 'link tx rx'
#       option dev 'eth0'
        option dev 'mesh-vpn'

config led 'led_lan'
        option name 'LAN'
        option sysfs 'gl-mifi:green:lan'
        option trigger 'netdev'
        option mode 'link tx rx'
        option dev 'eth1'

config led 'led_3gnet'
        option name '3GNET'
        option sysfs 'gl-mifi:green:net'
        option trigger 'netdev'
        option mode 'link tx rx'
        option dev 'wwan0'
